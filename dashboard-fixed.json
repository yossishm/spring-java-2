{
  "dashboard": {
    "id": null,
    "uid": "java-vs-dotnet-fixed",
    "title": "⚔️ Java vs .NET Performance (Dual Scale)",
    "tags": ["java", "dotnet", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate: Java vs .NET (Separate Scales)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {"drawStyle": "line", "lineWidth": 2}
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Java App"},
              "properties": [
                {"id": "custom.axisPlacement", "value": "left"},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": ".NET App"},
              "properties": [
                {"id": "custom.axisPlacement", "value": "right"},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "sum(rate(otel_http_server_requests_milliseconds_count{service_name=\"java-spring-app\"}[1m]))",
            "legendFormat": "Java App",
            "refId": "A"
          },
          {
            "expr": "sum(rate(otel_aspnetcore_routing_match_attempts_total{service_name=\"SpringJavaEquivalent\"}[1m]))",
            "legendFormat": ".NET App",
            "refId": "B"
          }
        ],
        "datasource": {"uid": "PBFA97CFB590B2093"}
      },
      {
        "id": 2,
        "title": "Total Requests (5m): Java vs .NET",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {"drawStyle": "line", "lineWidth": 2}
          }
        },
        "targets": [
          {
            "expr": "sum(increase(otel_http_server_requests_milliseconds_count{service_name=\"java-spring-app\"}[5m]))",
            "legendFormat": "Java App",
            "refId": "A"
          },
          {
            "expr": "sum(increase(otel_aspnetcore_routing_match_attempts_total{service_name=\"SpringJavaEquivalent\"}[5m]))",
            "legendFormat": ".NET App",
            "refId": "B"
          }
        ],
        "datasource": {"uid": "PBFA97CFB590B2093"}
      }
    ],
    "time": {"from": "now-5m", "to": "now"},
    "refresh": "5s"
  },
  "overwrite": true
}
