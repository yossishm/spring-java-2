.env.local
.env.production
.env.staging

# Secrets and keys
*.key
*.pem
*.p12
*.pfx
secrets/
keys/

# Logs
logs/
*.log

# Temporary files
tmp/
temp/
*.tmp

# IDE files
.vs/
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

