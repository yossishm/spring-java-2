{
  "dashboard": {
    "id": null,
    "title": "Clean App Traffic (No Health Checks)",
    "tags": ["clean", "app-only", "otlp"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Traffic Only (exclude health checks)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(increase(otel_http_server_requests_milliseconds_count{service_name=\"java-spring-app\",uri!~\"/actuator/health.*\"}[1m])) / 60",
            "legendFormat": "Java (no health checks)"
          },
          {
            "expr": "sum(increase(otel_http_server_request_duration_seconds_count{service_name=\"dotnet-app\",http_route!~\"/health/.*\"}[1m])) / 60",
            "legendFormat": "DotNet (no health checks)"
          }
        ],
        "fieldConfig": {"defaults": {"unit": "reqps"}},
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Health Check Traffic Only",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(increase(otel_http_server_requests_milliseconds_count{service_name=\"java-spring-app\",uri=~\"/actuator/health.*\"}[1m])) / 60",
            "legendFormat": "Java health checks"
          },
          {
            "expr": "sum(increase(otel_http_server_request_duration_seconds_count{service_name=\"dotnet-app\",http_route=~\"/health/.*\"}[1m])) / 60",
            "legendFormat": "DotNet health checks"
          }
        ],
        "fieldConfig": {"defaults": {"unit": "reqps"}},
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Total Traffic (All Routes)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(increase(otel_http_server_requests_milliseconds_count{service_name=\"java-spring-app\"}[1m])) / 60",
            "legendFormat": "Java total"
          },
          {
            "expr": "sum(increase(otel_http_server_request_duration_seconds_count{service_name=\"dotnet-app\"}[1m])) / 60",
            "legendFormat": "DotNet total"
          }
        ],
        "fieldConfig": {"defaults": {"unit": "reqps"}},
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0
  }
}
