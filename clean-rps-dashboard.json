{
  "dashboard": {
    "id": null,
    "title": "Clean RPS (Main route, 200)",
    "tags": ["rps", "clean", "otlp"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Req/sec (increase/60, main route 200)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(increase(otel_http_server_requests_milliseconds_count{service_name=\"java-spring-app\",uri=\"/\",status=\"200\"}[1m])) / 60",
            "legendFormat": "Java"
          },
          {
            "expr": "sum(increase(otel_http_server_request_duration_seconds_count{service_name=\"dotnet-app\",http_route=\"\",http_response_status_code=\"200\"}[1m])) / 60",
            "legendFormat": "DotNet"
          }
        ],
        "fieldConfig": {"defaults": {"unit": "reqps"}},
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 0}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0
  }
}
